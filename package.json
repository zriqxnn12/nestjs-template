    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/sequelize": "^10.0.1",
    "aws-sdk": "^2.1646.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "currency.js": "^2.0.4",
    "joi": "^17.13.3",
    "meilisearch": "^0.40.0",
    "mysql2": "^3.10.1",
    "npm-check-updates": "^16.14.20",
    "passport": "^0.7.0",
